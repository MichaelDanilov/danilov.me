{
  "name": "danilov.me",
  "version": "3.1.0",
  "description": "Personal website of Michael Danilov",
  "homepage": "https://github.com/MichaelDanilov/danilov.me",
  "repository": {
    "type": "git",
    "url": "https://github.com/MichaelDanilov/danilov.me.git"
  },
  "bugs": {
    "url": "https://github.com/MichaelDanilov/danilov.me/issues",
    "email": "michael@danilov.me"
  },
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf ./node_modules ./public",
    "lint:js": "eslint \"src/**/*.{js,ts,tsx}\"",
    "lint": "run-p lint:* spellcheck",
    "spellcheck": "yaspeller *.md",
    "start:dev": "NODE_PATH=./ gatsby develop",
    "start": "npm run start:dev",
    "test": "jest"
  },
  "author": {
    "name": "Michael Danilov",
    "email": "michael@danilov.me",
    "url": "https://danilov.me/"
  },
  "license": "MIT",
  "dependencies": {
    "acorn": "7.1.1",
    "gatsby": "2.17.7",
    "gatsby-plugin-favicon": "3.1.6",
    "gatsby-plugin-offline": "3.0.17",
    "gatsby-plugin-react-helmet": "3.1.13",
    "gatsby-plugin-sitemap": "2.2.19",
    "gatsby-plugin-styled-components": "3.1.11",
    "gatsby-plugin-typescript": "2.1.15",
    "graphql": "14.5.8",
    "michaeldanilov": "1.1.1",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-helmet": "5.2.1",
    "styled-components": "4.4.1",
    "typescript": "3.9.5"
  },
  "devDependencies": {
    "@types/jest": "25.2.3",
    "@types/react-helmet": "5.0.14",
    "@types/react-test-renderer": "16.9.1",
    "@types/styled-components": "4.1.20",
    "@typescript-eslint/parser": "2.34.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-node": "0.3.3",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-unicorn": "19.0.1",
    "husky": "4.2.5",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.0.1",
    "lint-staged": "10.2.9",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "react-test-renderer": "16.11.0",
    "request": "2.88.2",
    "ts-jest": "26.1.0",
    "yaspeller": "7.0.0"
  },
  "overrides": {
    "graphql": "14.1.1"
  },
  "resolutions": {
    "graphql": "14.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "./node_modules/.bin/eslint",
      "./node_modules/.bin/prettier --write"
    ],
    "*.md": [
      "./node_modules/.bin/yaspeller"
    ]
  }
}
