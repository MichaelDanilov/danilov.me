AddDefaultCharset utf-8
AddCharset utf-8 *
<IfModule mod_charset.c>
    CharsetSourceEnc utf-8
    CharsetDefault utf-8
</IfModule>
DirectoryIndex index.html
ErrorDocument 404 /
RewriteEngine on
RewriteCond %{HTTP_HOST} www.danilov.me$ [NC]
RewriteRule ^(.*)$ http://danilov.me/$1 [R=301,L]
RewriteCond %{HTTP_HOST} dnlv.ru$ [NC]
RewriteRule ^(.*)$ http://danilov.me/$1 [R=301,L]
RewriteCond %{HTTP_HOST} www.dnlv.ru$ [NC]
RewriteRule ^(.*)$ http://danilov.me/$1 [R=301,L]
RewriteCond %{HTTP_HOST} michael-danilov.ru$ [NC]
RewriteRule ^(.*)$ http://danilov.me/$1 [R=301,L]
RewriteCond %{HTTP_HOST} www.michael-danilov.ru$ [NC]
RewriteRule ^(.*)$ http://danilov.me/$1 [R=301,L]
RewriteCond %{HTTP_HOST} michael-danilov.com$ [NC]
RewriteRule ^(.*)$ http://danilov.me/$1 [R=301,L]
RewriteCond %{HTTP_HOST} www.michael-danilov.com$ [NC]
RewriteRule ^(.*)$ http://danilov.me/$1 [R=301,L]
